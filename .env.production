# Production environment variables for EAS builds
# This file ensures production builds skip unnecessary checks and telemetry

# Disable Expo Doctor (prevents build failures from non-critical warnings)
EXPO_NO_DOCTOR=true

# Disable Expo Telemetry (faster builds, no data collection)
EXPO_NO_TELEMETRY=true

# Set production environment
NODE_ENV=production

# Disable CocoaPods stats (faster pod install)
COCOAPODS_DISABLE_STATS=1
