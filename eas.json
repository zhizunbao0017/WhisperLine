{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "image": "latest",
        "buildConfiguration": "Release"
      },
      "env": {
        "COCOAPODS_DISABLE_STATS": "1",
        "EXPO_NO_DOCTOR": "true",
        "EXPO_NO_TELEMETRY": "true",
        "RCT_NO_LAUNCH_PACKAGER": "true",
        "NO_FLIPPER": "1",
        "NODE_ENV": "production"
      },
      "node": "18.18.2",
      "cache": {
        "disabled": false,
        "paths": [
          "node_modules/**",
          "ios/Pods/**",
          ".expo/**"
        ]
      }
    }
  },
  "submit": {
    "production": {}
  }
}
