{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "image": "latest",
        "buildConfiguration": "Release"
      },
      "env": {
        "COCOAPODS_DISABLE_STATS": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
