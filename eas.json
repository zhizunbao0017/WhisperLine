{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "image": "sdk-54",
        "buildConfiguration": "Release"
      },
      "env": {
        "COCOAPODS_DISABLE_STATS": "1",
        "EXPO_NO_DOCTOR": "false",
        "EXPO_NO_TELEMETRY": "true",
        "RCT_NO_LAUNCH_PACKAGER": "true",
        "NO_FLIPPER": "1",
        "EXPO_METRO_NO_SOURCE_MAPS": "true",
        "GENERATE_SOURCEMAP": "false",
        "EXPO_DEBUG": "false",
        "CI": "true",
        "SKIP_BUNDLING": "false",
        "EXPO_USE_METRO_WORKER": "1",
        "NODE_ENV": "production",
        "RCT_NEW_ARCH_ENABLED": "0"
      },
      "node": "18.18.2",
      "cache": {
        "disabled": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
